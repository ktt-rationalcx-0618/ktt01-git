{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ktt01-dev-adf"
		},
		"LS_ktt01_dev_CosmosDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ktt01_dev_CosmosDB'"
		},
		"LS_ktt01_dev_AzureSqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ktt01_dev_AzureSqlDB'"
		},
		"LS_ktt01_Dev_BlobStorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ktt01-dev-SA-AccessSecret"
		},
		"LS_ktt01_Dev_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ktt01-dev-kv-00.vault.azure.net/"
		},
		"ParametersEvenTrigger_properties_pl_parameter_IO_triggers_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().filename"
		},
		"ParametersEvenTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/9ff82384-d93c-4e95-8e8f-4d5b507efa11/resourceGroups/ktt01-dev-rg/providers/Microsoft.Storage/storageAccounts/ktt01devsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ktt01-dev-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ktt01_dev_CosmosDB_connectionString": {
						"value": "[parameters('LS_ktt01_dev_CosmosDB_connectionString')]"
					},
					"LS_ktt01_dev_AzureSqlDB_connectionString": {
						"value": "[parameters('LS_ktt01_dev_AzureSqlDB_connectionString')]"
					},
					"LS_ktt01_Dev_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_ktt01_Dev_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_ktt01_Dev_BlobStorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ktt01_Dev_BlobStorage_properties_typeProperties_connectionString_secretName')]"
					},
					"ParametersEvenTrigger_properties_pl_parameter_IO_triggers_parameters_filename": {
						"value": "[parameters('ParametersEvenTrigger_properties_pl_parameter_IO_triggers_parameters_filename')]"
					},
					"ParametersEvenTrigger_properties_typeProperties_scope": {
						"value": "[parameters('ParametersEvenTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}